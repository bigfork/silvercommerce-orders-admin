{
  "name": "silvercommerce/orders-admin",
  "description": "Silverstripe module for creating and managing invoice and estimate objects in the admin",
    "type": "silverstripe-vendormodule",
    "homepage": "https://github.com/silvercommerce/orders-admin",
    "keywords": [
        "silverstripe",
        "orders",
        "invoices",
        "estimates"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Morven Lewis-Everley",
            "email": "morven@ilateral.co.uk"
        },
        {
            "name": "Mark Anderson",
            "email": "mark@ilateral.co.uk"
        }
    ],
    "support": {
        "issues": "https://github.com/silvercommerce/orders-admin/issues"
    },
    "require": {
        "php": ">=7.1.0",
        "silverstripe/framework": "^5",
        "silverstripe/siteconfig": "^5",
        "silverstripe/admin": "^2",
        "silverstripe/versioned": "^2",
        "colymba/gridfield-bulk-editing-tools": "^4",
        "symbiote/silverstripe-gridfieldextensions": "^4",
        "dompdf/dompdf": "^3",
        "i-lateral/silverstripe-modeladminplus": "^2",
        "silvercommerce/settings": "dev-ss5",
        "silvercommerce/tax-admin": "dev-ss5",
        "silvercommerce/contact-admin": "dev-ss5",
        "silvercommerce/versionhistoryfield": "dev-ss5",
        "silvershop/silverstripe-hasonefield": "^4",
        "lekoala/silverstripe-uuid": "^1"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/bigfork/silvercommerce-settings"
        },
        {
            "type": "vcs",
            "url": "https://github.com/bigfork/silvercommerce-tax-admin"
        },
        {
            "type": "vcs",
            "url": "https://github.com/bigfork/silvercommerce-contact-admin"
        },
        {
            "type": "vcs",
            "url": "https://github.com/bigfork/silvercommerce-versionhistoryfield"
        }
    ],
    "extra": {
        "expose": [
            "client/dist"
        ]
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/vendor-plugin": true,
            "php-http/discovery": true,
            "silverstripe/recipe-plugin": true
        }
    },
    "autoload": {
        "psr-4": {
            "SilverCommerce\\OrdersAdmin\\": "src/",
            "SilverCommerce\\OrdersAdmin\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "lint": "vendor/bin/phpcs src/ tests/",
        "lint-clean": "vendor/bin/phpcbf src/ tests/"
    },
    "suggest": {
        "silverstripe/cms": "Add full content management support to your site",
        "i-lateral/silverstripe-users": "Add user account management, login and registration"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
